{
   "arrowParens": "always",
   "bracketSameLine": false,
   "bracketSpacing": true,
   "embeddedLanguageFormatting": "auto",
   "htmlWhitespaceSensitivity": "css",
   "insertPragma": false,
   "jsxSingleQuote": true,
   "tabWidth": 4,
   "proseWrap": "preserve",
   "quoteProps": "as-needed",
   "requirePragma": false,
   "semi": true,
   "singleQuote": true,
   "trailingComma": "all",
   "useTabs": false,
   "vueIndentScriptAndStyle": false,
   "overrides": [
      {
        "files": "*.hbs",
        "options": {
          "singleQuote": false
        }
      }
    ]
}
